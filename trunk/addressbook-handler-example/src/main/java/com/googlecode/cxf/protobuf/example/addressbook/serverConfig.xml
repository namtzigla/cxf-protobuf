<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean class="com.googlecode.cxf.protobuf.ProtobufServerFactoryBean"
		init-method="create">
		<property name="address" value="http://localhost:8888/AddressBookService" />
		<property name="serviceBean">
			<bean class="com.googlecode.cxf.protobuf.example.addressbook.AddressBookServiceImpl" />
		</property>
		<property name="messageClass"
			value="com.example.tutorial.AddressBookProtos$AddressBookServiceMessage" />
	</bean>

</beans>